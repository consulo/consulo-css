<idea-plugin>
	<extensions defaultExtensionNs="org.consulo.emmet">
		<optionsProvider id="css.emmet.application.settings"
						 displayName="CSS"
						 instance="com.intellij.application.options.emmet.CssEmmetConfigurable"/>
	</extensions>

	<extensions defaultExtensionNs="com.intellij">
		<applicationService serviceInterface="com.intellij.application.options.emmet.CssEmmetOptions"
							serviceImplementation="com.intellij.application.options.emmet.CssEmmetOptions"/>
	</extensions>
</idea-plugin>