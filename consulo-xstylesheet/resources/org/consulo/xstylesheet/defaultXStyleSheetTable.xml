<?xml version="1.0" encoding="UTF-8"?>
<xstyle-sheet-table xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
					xsi:noNamespaceSchemaLocation="defaultXStyleSheetTable.xsd">
	<!--http://www.w3.org/TR/CSS21/propidx.html-->
	<value-definition name="color" implClass="org.consulo.xstylesheet.definition.value.impl.ColorXStyleSheetValue"/>
	<value-definition name="colorKeyword" implClass="org.consulo.xstylesheet.definition.value.impl.KeywordColorXStyleSheetValue"/>
	<value-definition name="keyword" implClass="org.consulo.xstylesheet.definition.value.impl.KeywordXStyleSheetValue"/>
	<value-definition name="number" implClass="org.consulo.xstylesheet.definition.value.impl.NumberXStyleSheetValue"/>
	<value-definition name="percentNumber" implClass="org.consulo.xstylesheet.definition.value.impl.PercentNumberXStyleSheetValue"/>
	<value-definition name="relativeLengthNumber" implClass="org.consulo.xstylesheet.definition.value.impl.RelativeLengthNumberXStyleSheetValue"/>
	<value-definition name="absoluteLengthNumber" implClass="org.consulo.xstylesheet.definition.value.impl.AbsoluteLengthNumberXStyleSheetValue"/>
	<value-definition name="function" implClass="org.consulo.xstylesheet.definition.value.impl.FunctionXStyleSheetValue"/>

	<property name="background-color">
		<valid-values>
			<entry>
				<color/>
				<colorKeyword/>
				<keyword>transparent</keyword>
				<keyword>inherit</keyword>
			</entry>
		</valid-values>
		<initial-values>
			<entry>
				<keyword>transparent</keyword>
			</entry>
		</initial-values>
	</property>
	<property name="color">
		<valid-values>
			<entry>
				<color/>
				<colorKeyword/>
				<keyword>inherit</keyword>
			</entry>
		</valid-values>
		<initial-values>
			<entry>
				<keyword>inherit</keyword>
			</entry>
		</initial-values>
	</property>
	<property name="background-image">
		<valid-values>
			<entry>
				<function />
				<keyword>inherit</keyword>
				<keyword>none</keyword>
			</entry>
		</valid-values>
		<initial-values>
			<entry>
				<keyword>inherit</keyword>
			</entry>
		</initial-values>
	</property>
	<property name="background-repeat">
		<valid-values>
			<entry>
				<keyword>repeat</keyword>
				<keyword>repeat-x</keyword>
				<keyword>repeat-y</keyword>
				<keyword>no-repeat</keyword>
				<keyword>inherit</keyword>
			</entry>
		</valid-values>
		<initial-values>
			<entry>
				<keyword>repeat</keyword>
			</entry>
		</initial-values>
	</property>
	<property name="visibility">
		<valid-values>
			<entry>
				<keyword>visible</keyword>
				<keyword>hidden</keyword>
				<keyword>collapse</keyword>
				<keyword>inherit</keyword>
			</entry>
		</valid-values>
		<initial-values>
			<entry>
				<keyword>visible</keyword>
			</entry>
		</initial-values>
	</property>
	<property name="font-weight">
		<valid-values>
			<entry>
				<keyword>normal</keyword>
				<keyword>bold</keyword>
				<keyword>bolder</keyword>
				<keyword>lighter</keyword>
				<keyword>inherit</keyword>
				<number>100</number>
				<number>200</number>
				<number>300</number>
				<number>400</number>
				<number>500</number>
				<number>600</number>
				<number>700</number>
				<number>800</number>
				<number>900</number>
			</entry>
		</valid-values>
		<initial-values>
			<entry>
				<keyword>normal</keyword>
			</entry>
		</initial-values>
	</property>
	<property name="font-style">
		<valid-values>
			<entry>
				<keyword>normal</keyword>
				<keyword>italic</keyword>
				<keyword>oblique</keyword>
				<keyword>inherit</keyword>
			</entry>
		</valid-values>
		<initial-values>
			<entry>
				<keyword>normal</keyword>
			</entry>
		</initial-values>
	</property>
	<property name="font-variant">
		<valid-values>
			<entry>
				<keyword>normal</keyword>
				<keyword>small-caps</keyword>
				<keyword>inherit</keyword>
			</entry>
		</valid-values>
		<initial-values>
			<entry>
				<keyword>normal</keyword>
			</entry>
		</initial-values>
	</property>
	<property name="float">
		<valid-values>
			<entry>
				<keyword>left</keyword>
				<keyword>right</keyword>
				<keyword>none</keyword>
				<keyword>inherit</keyword>
			</entry>
		</valid-values>
		<initial-values>
			<entry>
				<keyword>none</keyword>
			</entry>
		</initial-values>
	</property>
	<property name="empty-cells">
		<valid-values>
			<entry>
				<keyword>show</keyword>
				<keyword>hide</keyword>
				<keyword>inherit</keyword>
			</entry>
		</valid-values>
		<initial-values>
			<entry>
				<keyword>show</keyword>
			</entry>
		</initial-values>
	</property>
	<property name="background-attachment">
		<valid-values>
			<entry>
				<keyword>scroll</keyword>
				<keyword>fixed</keyword>
				<keyword>inherit</keyword>
			</entry>
		</valid-values>
		<initial-values>
			<entry>
				<keyword>scroll</keyword>
			</entry>
		</initial-values>
	</property>
	<property name="border-collapse">
		collapse | separate | inherit
		<valid-values>
			<entry>
				<keyword>collapse</keyword>
				<keyword>separate</keyword>
				<keyword>inherit</keyword>
			</entry>
		</valid-values>
		<initial-values>
			<entry>
				<keyword>separate</keyword>
			</entry>
		</initial-values>
	</property>
	<property name="border-top-color">
		<valid-values>
			<entry>
				<color/>
				<colorKeyword/>
				<keyword>transparent</keyword>
				<keyword>inherit</keyword>
			</entry>
		</valid-values>
	</property>
	<property name="border-right-color">
		<valid-values>
			<entry>
				<color/>
				<colorKeyword/>
				<keyword>transparent</keyword>
				<keyword>inherit</keyword>
			</entry>
		</valid-values>
	</property>
	<property name="border-bottom-color">
		<valid-values>
			<entry>
				<color/>
				<colorKeyword/>
				<keyword>transparent</keyword>
				<keyword>inherit</keyword>
			</entry>
		</valid-values>
	</property>
	<property name="border-left-color">
		<valid-values>
			<entry>
				<color/>
				<colorKeyword/>
				<keyword>transparent</keyword>
				<keyword>inherit</keyword>
			</entry>
		</valid-values>
	</property>
</xstyle-sheet-table>